[
  {
    "description": "Classify objects in a single image",
    "method": "POST",
    "url": "/image/classify",
    "controller": "image",
    "action": "singleImageClassify",
    "params": [
      {
        "name": "image",
        "required": true,
        "type": "file",
        "default": false
      },
      {
        "name": "classes",
        "required": false,
        "type": "classes",
        "default": [],
        "example": ["vehicles", "faces"]
      },
      {
        "name": "annotate",
        "required": false,
        "type": "classes",
        "default": [],
        "example": ["vehicles", "faces"]
      },
      {
        "name": "blur",
        "required": false,
        "type": "classes",
        "default": [],
        "example": ["vehicles", "faces"]
      }
    ]
  }
]